<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false">
	<diskStore path="java.io.tmpdir" />

	<!-- objects are evicted from the cache every 600 seconds -->


	<cache name="provinces" timeToLiveSeconds="600"
		maxElementsInMemory="100000" />

	<cache name="citys" timeToLiveSeconds="600" maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.Application"
		timeToLiveSeconds="600" maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.AppParameter"
		timeToLiveSeconds="600" maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.Bonus" timeToLiveSeconds="600"
		maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.Check" timeToLiveSeconds="600"
		maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.Dictionary"
		timeToLiveSeconds="600" maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.Sdk" timeToLiveSeconds="600"
		maxElementsInMemory="100000" />

	<cache name="com.rainasmoon.onepay.model.User" timeToLiveSeconds="600"
		maxElementsInMemory="100000" />

</ehcache>
